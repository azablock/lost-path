<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lp.view_controller.ui.MenuPaneController">
  <children>
    <BorderPane fx:id="menuPane" prefHeight="800.0" prefWidth="1280.0">
      <center>
        <GridPane BorderPane.alignment="CENTER">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
            <Button mnemonicParsing="false" onMouseClicked="#onStartClicked" prefHeight="40.0" prefWidth="200.0" text="Start" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <Button mnemonicParsing="false" onMouseClicked="#onExitClicked" prefHeight="40.0" prefWidth="200.0" text="Exit" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="85.0" prefWidth="440.0" styleClass="menu-label" text="THE LOST PATH" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
          </children>
        </GridPane>
      </center>
    </BorderPane>
  </children>
</Pane>
